{
    "extends": "eslint:recommended",
    "parserOptions": {
        "ecmaVersion": 6
    },
    "env": {
        "node": true,
        "browser": true,
        "es6": true,
        "jquery": true
    },
    "rules": {
        "comma-dangle": "off",
        "no-unused-vars": [
            "error", {
                "vars": "local",
                "args": "none"
            }
        ]
    }
}